<?xml version="1.0"?>
<project name="rmi_compile" default="rmic" basedir=".">


  <!-- Standard Directory Identifiers-->
  <property name="PSE" value="${basedir}"/>


  <!-- RMI Target -->
  <target name="rmic" >
    <rmic base="${PSE}/bin" classname="org.ojim.rmi.server.ImplNetOjim"/>
  	<rmic base="${PSE}/bin" classname="org.ojim.rmi.client.ImplNetClient"/>
    <copy file ="bin/org/ojim/rmi/server/ImplNetOjim_Stub.class" todir="bin/org/ojim/rmi/client"/>
  	<copy file ="bin/org/ojim/rmi/server/ImplNetOjim_Skel.class" todir="bin/org/ojim/rmi/client"/>
  	<copy file ="bin/org/ojim/rmi/client/ImplNetClient_Stub.class" todir="bin/org/ojim/rmi/server"/>
  	<copy file ="bin/org/ojim/rmi/client/ImplNetClient_Skel.class" todir="bin/org/ojim/rmi/server"/>
  	 
  </target>
	
	
	
	


</project>